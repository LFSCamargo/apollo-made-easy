{
  "name": "react-ts-boilerplate",
  "version": "0.1.0",
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "apollo": "^2.3.1",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-client-preset": "^1.0.8",
    "apollo-codegen": "^0.20.2",
    "apollo-link-context": "^1.0.12",
    "apollo-link-ws": "^1.0.12",
    "apollo-utilities": "^1.1.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "gravatar": "^1.8.0",
    "idx": "^2.5.2",
    "react": "16.6.0",
    "react-apollo": "^2.4.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1",
    "styled-components": "4.0.2",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/gravatar": "^1.4.28",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-router": "4.0.32",
    "@types/react-router-dom": "4.3.1",
    "@types/styled-components": "4.0.2",
    "@types/ws": "^6.0.1",
    "gh-pages": "2.0.1",
    "react-scripts-ts": "3.1.0",
    "typescript": "3.1.3"
  },
  "private": true,
  "scripts": {
    "update-schema": "cd data && apollo-codegen introspect-schema http://localhost:5000 --output schema.json",
    "codegen": "apollo codegen:generate --localSchemaFile=data/schema.json --target=typescript && rm -rf __generated__",
    "build": "react-scripts-ts build",
    "deploy": "yarn build && gh-pages -d build",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --fix 'src/**/*.ts*'",
    "start": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom"
  }
}
